{
  "name": "DataXylo",
  "version": "1.0.0",
  "scripts": {
    "start": "node server.js",
    "build": "BABEL_ENV=production ./node_modules/.bin/webpack --config webpack.config.production.js",
    "clean:dist": "rimraf dist",
    "clean:js": "rimraf public/js/main*.js && rimraf public/js/app.js && rimraf public/js/plugins.js && rimraf public/js/*.map",
    "clean:css": "rimraf public/css/main*.css && rimraf public/css/app*.css && rimraf public/css/*.map",
    "clean:fonts": "rimraf public/fonts/*",
    "clean:server": "rimraf server.js && rimraf server.js.map",
    "clean:tmp": "rimraf tmp",
    "clean": "run-s clean:dist clean:tmp clean:server clean:css clean:js clean:fonts",
    "update:schema": "babel-node tools/update-schema.js",
    "load-sample": "babel-node tools/load-sample-data.js",
    "load-aggregation": "babel-node tools/load-aggregates-data.js",
    "load-elastic": "babel-node tools/load-elastic.js",
    "loader:reseedTenants": "babel-node data-loader/loader/loader.js --reseedTenants",
    "loader": "babel-node data-loader/loader/loader.js",
    "hot:client": "cross-env NODE_ENV=development EXTRACT_TEXT_PLUGIN=false webpack-dev-server --config tools/webpack.client.js",
    "hot:server": "cross-env NODE_ENV=development webpack --colors --display-error-details --watch --verbose --config tools/webpack.server.js",
    "dev:server": "cross-env NODE_ENV=development node tmp/bundle.js",
    "dev:server:wait": "just-wait -p \"tmp/bundle.js\" --timeout 120 && npm run start-watcher",
    "start-watcher": "npm-watch",
    "watch-build-server": "run-s clean:tmp hot:server",
    "dev": "rimraf tmp && concurrently --kill-others \"npm run watch-build-server\" \"npm run dev:server:wait\"",
    "dev:rtl": "cross-env RTL=true run-s dev",
    "dev:dist": "run-p dev dist:watch",
    "build:server": "cross-env NO_OUTPUT_PATH=true NODE_ENV=production webpack --progress --colors --display-error-details --config tools/webpack.server.js",
    "build:client": "cross-env EXTRACT_TEXT_PLUGIN=true webpack --progress --colors --display-error-details --config tools/webpack.client.js",
    "bundle:server": "run-s clean:tmp clean:server build:server",
    "bundle:client": "run-s clean:css clean:js clean:fonts build:client",
    "prod:server": "cross-env NODE_ENV=production node server.js",
    "build:prod": "cross-env NODE_ENV=production run-s bundle:client bundle:server",
    "build:prod:rtl": "cross-env NODE_ENV=production RTL=true run-s bundle:client bundle:server",
    "prod": "run-s build:prod prod:server",
    "prod:rtl": "run-s build:prod:rtl prod:server",
    "dist": "cross-env NODE_ENV=production run-s bundle:client && node tools/create-dist.js",
    "dist:watch": "run-s bundle:client && node tools/watch-dist.js",
    "test": "mocha --compilers :./tools/babel-setup.js --recursive ./test/*.js",
    "start_server": "node server/app.js"
  },
  "keywords": [
    "inspinia",
    "react",
    "reactjs",
    "webpack",
    "data"
  ],
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin-loader": "^0.9.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "path": "^0.12.7",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "animate.css": "^3.1.1",
    "async": "^2.1.4",
    "async-file": "^2.0.2",
    "bcryptjs": "2.3.0",
    "body-parser": "1.15.2",
    "bootstrap": "^3.3.7",
    "bootstrap-datepicker": "^1.6.4",
    "chart.js": "^2.4.0",
    "command-line-args": "^4.0.1",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "crypto": "0.0.3",
    "csvtojson": "^1.1.1",
    "d3": "^4.4.0",
    "d3-funnel": "^1.0.0",
    "datamaps": "^0.5.8",
    "datatables": "^1.10.12",
    "datatables.net": "^1.10.12",
    "datatables.net-bs": "^1.10.12",
    "datatables.net-buttons": "^1.2.2",
    "datatables.net-buttons-bs": "^1.2.2",
    "datatables.net-buttons-dt": "^1.2.2",
    "datatables.net-dt": "^1.10.12",
    "datatables.net-select": "^1.2.0",
    "datatables.net-select-bs": "^1.2.0",
    "daterangepicker": "0.0.3",
    "dom-tools": "^0.1.4",
    "ejs": "2.5.2",
    "elasticsearch": "12.1.0",
    "errorhandler": "1.5.0",
    "express": "4.14.0",
    "file-extension": "^3.1.0",
    "font-awesome": "^4.6.1",
    "history": "^3.0.0",
    "icheck": "^1.0.2",
    "identicons-react": "^0.1.0",
    "isomorphic-fetch": "2.2.1",
    "jquery": "^2.2.3",
    "jquery-slimscroll": "^1.3.6",
    "jquery-ui": "^1.12.1",
    "jsftp": "1.5.5",
    "jsonminify": "^0.4.1",
    "jsonwebtoken": "7.1.9",
    "jwt-simple": "0.5.1",
    "lodash": "^4.17.2",
    "method-override": "2.3.7",
    "metismenu": "^2.5.0",
    "moment": "^2.17.0",
    "mongoose": "4.7.0",
    "morgan": "1.7.0",
    "multer": "1.2.0",
    "node-async-loop": "^1.2.2",
    "node-json-minify": "^1.0.0",
    "promised-exec": "^1.0.1",
    "promised-mongo": "1.2.0",
    "pubsub-js": "1.5.4",
    "react": "^0.14.6",
    "react-bootstrap": "^0.28.3",
    "react-bubble-chart": "^0.4.0",
    "react-dom": "^0.14.6",
    "react-dropzone": "^3.7.3",
    "react-dropzone-component": "^1.2.1",
    "react-icheck": "^0.3.6",
    "react-input-range": "^0.9.3",
    "react-pace": "^1.0.0",
    "react-router": "^2.7.0",
    "react-scrollbar": "^0.5.1",
    "react-sparklines": "^1.6.0",
    "request": "2.79.0",
    "resumer": "0.0.0",
    "searchkit": "0.10.1",
    "select2": "^4.0.3",
    "sha256": "0.2.0",
    "socket.io": "1.6.0",
    "socketio-jwt": "4.5.0",
    "strformat": "0.0.7",
    "underscore": "^1.8.3",
    "venn.js": "^0.2.14",
    "vex": "0.0.4",
    "webhdfs": "1.1.0",
    "whatwg-fetch": "2.0.1",
    "yesno": "0.0.1"
  }
}
